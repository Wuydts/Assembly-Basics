%include "/usr/share/csc314/asm_io.inc"


segment .data


segment .bss


segment .text
	global  asm_main

asm_main:
	enter	0,0
	pusha
	;***************CODE STARTS HERE***************************


push	123
call	myfunc


	;***************CODE ENDS HERE*****************************
	popa
	mov	eax, 0
	leave
	ret


myfunc:
	pop		eax ; this stores the return address
	pop		ebx ; pop the value into ebx
	push	eax ; store the return address back in the stack

	ret